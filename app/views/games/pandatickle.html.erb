<% content_for :head do %>
    <title>Panda Tickle: Game</title>
    <%= stylesheet_link_tag "games", :media => "all" %>
<% end %>
<% content_for :center do %>
<audio id="gamemusic" class="gamesound">
  <source src= "<%= asset_path('FreedomDance.wav') %>" type="audio/wav">
  <source src= "<%= asset_path('Freedomdance.mp3') %>" type="audio/mp3">
</audio>
    <audio id="pandalaugh">
      <source src="<%= asset_path('heehee.wav') %>" type="audio/wav">
      <source src="<%= asset_path('heehee.mp3') %>" type="audio/mp3">
    </audio>
<% end %>
<% content_for :right do %>
<%= render "form" %>
    <br>
    <h2><span class="head2">Leaders</span></h2>
    <br>
    <%= will_paginate %>
    <table>
      <tr>
        <th>Player</th>
        <th>Score</th>
      </tr>
      <% @games.each do |games| %>
          <tr>
            <td><%= games.player %></td>
            <td><%= games.score %></td>
          </tr>
          <tr>
          </tr>
      <% end %>
    </table>
<% end %>

<% content_for :scripts do %>
    <% javascript_include_tag "pandatickle.js.erb" %>
<% end %>

